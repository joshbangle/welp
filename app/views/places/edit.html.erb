<div class="new-box col-10 offset-1 text-center">
<h1>Edit your place</h1>
<br />
<%= simple_form_for @place do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description, :as => :text, :input_html => {:rows => 3} %>
  <br />
  <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>
</div>